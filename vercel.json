{
    "version": 2,
    "builds": [
      {
        "src": "contact/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "contact/wsgi.py",
        "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"]
      }
    ]
  }